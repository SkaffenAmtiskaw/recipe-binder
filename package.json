{
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "test": "ava --watch",
    "coverage": "c8 --reporter=html --all --include=src/** --exclude=src/firebase/** --exclude=src/**/*.test.ts ava",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@vitejs/plugin-react": "^3.1.0",
    "dot-prop": "^6.0.1",
    "evergreen-ui": "^7.1.4",
    "firebase": "^8.2.9",
    "klona": "^2.0.5",
    "param-case": "^3.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "vite": "^4.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "^11.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@web/test-runner": "^0.12.0",
    "ava": "^5.2.0",
    "c8": "^7.13.0",
    "prettier": "^2.8.4",
    "prettier-plugin-organize-imports": "^3.2.2",
    "tsx": "^3.12.6",
    "typescript": "^5.0.2"
  },
  "ava": {
    "files": [
      "src/**/*.test.{ts,tsx}"
    ],
    "extensions": {
      "ts": "module",
      "tsx": "module"
    },
    "nodeArguments": [
      "--no-warnings",
      "--loader=tsx",
      "--experimental-specifier-resolution=node"
    ]
  },
  "type": "module"
}
